{
   "extends": ["eslint:recommended", "prettier", "prettier/react"],
   "plugins": ["react/recommended", "react", "react-hooks/recommended"],
   "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true
      }
   },
   "env": {
      "es6": true,
      "browser": true,
      "node": true
   },
   "rules": {
      "no-console": "off",
      "react-hooks/rules-of-hooks": "error"
      // "react/jsx-uses-react": "error",
      // "react/jsx-uses-vars": "error"
   }
}
