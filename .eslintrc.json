{
	"extends": ["eslint:recommended", "prettier", "prettier/react"],
	"plugins": [],
	"parserOptions": {
	"ecmaVersion": 2018,
	"sourceType": "module",
	"ecmaFeatures": {
		"jsx": true
	}
},
	"env": {
	"es6": true,
	"browser": true,
	"node": true
},
	// https://github.com/babel/babel-eslint
	"parser": "babel-eslint",
	"rules": {
	// https://eslint.org/docs/2.0.0/rules/strict
	"strict": 0
	},
	"settings": {
	"react": {
		"version": "detect"
	}
}
}
